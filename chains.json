layout: none
permalink: chains.json
---
[{% for chain in site.chains %}
  {
    "name": "{{ chain.name }}",
    "chain_id": "{{ chain.slug }}",
    "short_name": "{{ chain.short_name }}",
    "chain": "{{ chain.chain }}",
    "network": "{{ chain.network }}",
    "network_id": "{{ chain.network_id }}"
  }{% unless forloop.last %},{% endunless %}{% endfor %}
]